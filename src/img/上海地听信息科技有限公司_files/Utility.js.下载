//字典类
var DicClass = function (name, value, value2) {
    this.myName = name;
    this.myValue = value;
    this.myValue2 = value2;
};
//字典LIST类
var ListDic = function () {
    this.data = [];
    this.add = function (dicClass) {
        this.data[this.data.length] = dicClass;
    };
    this.getValue = function (dicName) {
        var result;
        for (var i = 0; i < this.data.length; i++) {
            if (this.data[i] != null) {
                if (this.data[i].myName == dicName) {
                    result = this.data[i].myValue;
                    break;
                }
            }
        }
        return result;
    };
    this.getValue2 = function (dicName) {
        var result;
        for (var i = 0; i < this.data.length; i++) {
            if (this.data[i] != null) {
                if (this.data[i].myName == dicName) {
                    result = this.data[i].myValue2;
                    break;
                }
            }
        }
        return result;
    };
    this.getName = function (dicValue) {
        var result;
        for (var i = 0; i < this.data.length; i++) {
            if (this.data[i] != null) {
                if (this.data[i].myValue == dicValue) {
                    result = this.data[i].myName;
                    break;
                }
            }
        }
        return result;
    }
};
//List类
var List = function (id) {
    this.data = [];
    this.id = id;
    this.add = function (theList) {
        this.data[this.data.length] = theList;
    };
    this.GetListByID = function (theId) {
        var result;
        for (var i = 0; i < this.data.length; i++) {
            if (this.data[i] != null) {
                if (this.data[i].id == theId) {
                    result = this.data[i];
                    break;
                }
            }
        }
        return result;
    }
};